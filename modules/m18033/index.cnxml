    </m:mtr>
    <m:mtr>
     <m:mtd>
      <m:mrow>
       <m:mn>9</m:mn><m:mi>y</m:mi><m:mo>+</m:mo><m:mn>20</m:mn><m:mo mathcolor="#3366ff">−</m:mo><m:mn mathcolor="#3366ff">20</m:mn>
