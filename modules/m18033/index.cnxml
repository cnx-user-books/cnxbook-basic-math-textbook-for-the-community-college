      </m:mrow>
     </m:mtd>
     <m:mtd columnalign="left">
      <m:mrow>
       <m:mtext mathvariant="italic">Add same side like terms.</m:mtext>
      </m:mrow>
     </m:mtd>
    </m:mtr>
    <m:mtr>
     <m:mtd>
      <m:mrow>
       <m:mn>15</m:mn><m:mi>x</m:mi><m:mo>+</m:mo><m:mn>8</m:mn>
      </m:mrow>
     </m:mtd>
     <m:mtd>
      <m:mo>=</m:mo>
     </m:mtd>
     <m:mtd>
      <m:mrow>
       <m:mo>−</m:mo><m:mn>2</m:mn><m:mo>+</m:mo><m:mn>14</m:mn><m:mi>x</m:mi>
      </m:mrow>
     </m:mtd>
     <m:mtd>
      <m:mrow/>
     </m:mtd>
    </m:mtr>
    <m:mtr>
     <m:mtd>
      <m:mrow>
       <m:mn>15</m:mn><m:mi>x</m:mi><m:mo>+</m:mo><m:mn>8</m:mn><m:mo mathcolor="#3366ff">−</m:mo><m:mn mathcolor="#3366ff">14</m:mn><m:mi mathcolor="#3366ff">x</m:mi>
      </m:mrow>
     </m:mtd>
     <m:mtd>
      <m:mo>=</m:mo>
     </m:mtd>
     <m:mtd>
      <m:mrow>
       <m:mo>−</m:mo><m:mn>2</m:mn><m:mo>+</m:mo><m:mn>14</m:mn><m:mi>x</m:mi><m:mo mathcolor="#3366ff">−</m:mo><m:mn mathcolor="#3366ff">14</m:mn><m:mi mathcolor="#3366ff">x</m:mi>
      </m:mrow>
     </m:mtd>
     <m:mtd columnalign="left">
      <m:mrow>
       <m:mtext mathvariant="italic">Subtract 14x on both sides.</m:mtext>
      </m:mrow>
     </m:mtd>
    </m:mtr>
    <m:mtr>
     <m:mtd>
      <m:mrow>
       <m:mi>x</m:mi><m:mo>+</m:mo><m:mn>8</m:mn>
      </m:mrow>
     </m:mtd>
     <m:mtd>
      <m:mo>=</m:mo>
     </m:mtd>
     <m:mtd>
      <m:mrow>
       <m:mo>−</m:mo><m:mn>2</m:mn>
      </m:mrow>
