     <m:mtd>
      <m:mrow/>
     </m:mtd>
    </m:mtr>
    <m:mtr>
     <m:mtd>
      <m:mrow>
       <m:mi>x</m:mi><m:mo>+</m:mo><m:mn>8</m:mn><m:mo mathcolor="#3366ff">−</m:mo><m:mn mathcolor="#3366ff">8</m:mn>
      </m:mrow>
     </m:mtd>
     <m:mtd>
      <m:mo>=</m:mo>
     </m:mtd>
     <m:mtd>
      <m:mrow>
       <m:mo>−</m:mo><m:mn>2</m:mn><m:mo mathcolor="#3366ff">−</m:mo><m:mn mathcolor="#3366ff">8</m:mn>
      </m:mrow>
