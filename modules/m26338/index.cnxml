        <exercise id="id1172310715464">
          <problem id="oo-id1172310394899">
            <para id="id1172314657318"><m:math><m:semantics><m:mrow><m:mstyle fontsize="12pt"><m:mrow><m:mrow><m:mo stretchy="false">−</m:mo><m:mrow><m:mo stretchy="false">∣</m:mo><m:mn>1</m:mn><m:mo stretchy="false">∣</m:mo></m:mrow></m:mrow></m:mrow></m:mstyle><m:mrow/></m:mrow><m:annotation encoding="StarMath 5.0"> size 12{- lline 1 rline } {}</m:annotation></m:semantics></m:math> = __</para>
          </problem>
          <solution id="oo-id1172310538160">
            <para id="id8364588">−1</para>
          </solution>
        </exercise>
        <exercise id="id1172314458949">
          <problem id="oo-id1172310501004">
            <para id="id1172310760144"><m:math><m:semantics><m:mrow><m:mstyle fontsize="12pt"><m:mrow><m:mrow><m:mo stretchy="false">−</m:mo><m:mrow><m:mo stretchy="false">∣</m:mo><m:mrow><m:mo stretchy="false">−</m:mo><m:mtext>52</m:mtext></m:mrow><m:mo stretchy="false">∣</m:mo></m:mrow></m:mrow></m:mrow></m:mstyle><m:mrow/></m:mrow><m:annotation encoding="StarMath 5.0"> size 12{- lline -"52" rline } {}</m:annotation></m:semantics></m:math> = __</para>
          </problem>
