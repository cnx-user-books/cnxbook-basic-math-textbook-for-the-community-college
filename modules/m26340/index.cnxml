      <section id="id7659894">
        <title>Exponential Law 6</title>
        <equation id="id1168126390754">
          <m:math>
            <m:semantics>
              <m:mrow>
                <m:mstyle fontsize="12pt">
                  <m:mrow>
                    <m:mrow>
