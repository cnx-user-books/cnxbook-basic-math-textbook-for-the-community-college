                              <m:mi/>
                              <m:mo stretchy="false">⋅</m:mo>
                              <m:mi/>
                            </m:mrow>
                            <m:mn>2</m:mn>
                            <m:mrow>
                              <m:mi/>
                              <m:mo stretchy="false">⋅</m:mo>
                              <m:mi/>
                            </m:mrow>
                            <m:mn>2</m:mn>
                            <m:mrow>
                              <m:mi/>
                              <m:mo stretchy="false">⋅</m:mo>
                              <m:mi/>
                            </m:mrow>
                            <m:mn>2</m:mn>
                            <m:mrow>
                              <m:mi/>
                              <m:mo stretchy="false">⋅</m:mo>
                              <m:mi/>
                            </m:mrow>
                            <m:mn>2</m:mn>
                            <m:mrow>
                              <m:mi/>
                              <m:mo stretchy="false">⋅</m:mo>
                              <m:mi/>
                            </m:mrow>
                            <m:mn>2</m:mn>
                            <m:mrow>
                              <m:mi/>
                              <m:mo stretchy="false">⋅</m:mo>
                              <m:mi/>
                            </m:mrow>
                            <m:mn>2</m:mn>
                          </m:mrow>
                        </m:mfrac>
                        <m:mrow/>
                      </m:mrow>
                    </m:mtr>
                    <m:mtr>
                      <m:mrow>
                        <m:mrow>
                          <m:mi/>
                          <m:mo stretchy="false">=</m:mo>
                          <m:mfrac>
                            <m:mn>1</m:mn>
                            <m:mrow>
                              <m:mrow>
                                <m:mrow>
                                  <m:mrow>
                                    <m:mrow>
                                      <m:mrow>
                                        <m:mn>2</m:mn>
                                        <m:mo stretchy="false">⋅</m:mo>
                                        <m:mn>2</m:mn>
                                      </m:mrow>
                                      <m:mo stretchy="false">⋅</m:mo>
                                      <m:mn>2</m:mn>
